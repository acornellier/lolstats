      "eslint --fix"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "2.8.2",
    "@prisma/client": "3.3.0",
    "blitz": "0.41.1",
    "react": "alpha",
    "react-dom": "alpha",
    "react-hook-form": "7.18.0",
    "twisted": "1.46.3",
    "zod": "3.11.6"
  },
  "devDependencies": {
    "@types/react": "17.0.33",
    "eslint": "7.32.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "husky": "6.0.0",
    "lint-staged": "10.5.4",
    "prettier": "2.4.1",
    "prettier-plugin-prisma": "0.15.0",
    "pretty-quick": "3.1.1",
    "prisma": "3.3.0",
    "typescript": "~4.3"
  },
  "private": true
